.type groupID <:symbol
.type artifactID <:symbol
.type version <:symbol

.decl Edges(xa: groupID, xb:artifactID, xc: version, ya: groupID, yb: artifactID, yc: version, deptype: symbol)
.input Edges(filename="Edges.facts")
.decl Origin(a: groupID, b:artifactID, c:version)

Origin(a, b ,c) :- Edges(a, b ,c , _ , _, _, _), !Edges(_, _, _, a, b, c, _).

.output Origin
